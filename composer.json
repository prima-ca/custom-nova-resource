{
    "name": "prima-ca/custom-nova-resource",
    "description": "Template for a custom Nova Resource",
    "keywords": [
        "laravel",
        "nova",
        "resource",
        "custom",
        "template"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Cyrus\\Nova\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": []
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
